{
    "name": "wordwa11",
    "author": "pxtrez",
    "description": "Works for private quizzes, also on 'play' and 'resource' modes || Made by pxtrez || Discord: doggo pxtrez#9100.",
    "version": "2.5",
    "background": {
        "content_scripts": [{
            "matches": [
                "*://*.wordwall.net*"
            ],
            "scripts": ["./src/script.js"]
        }]
    },
    "browser_action": {
        "default_icon": "./img/wordwa11.png",
        "default_popup": "./src/popup.html",
        "default_title": "wordwa11"
    },
    "icons": {
        "128": "img/wordwa11.png"
    },
    "permissions": [
        "tabs",
        "webRequest",
        "<all_urls>",
        "activeTab"
    ],
    "manifest_version": 2
}